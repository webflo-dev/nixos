{
  "root": true,
  "plugins": ["prettier"],
  "extends": ["eslint:recommended", "prettier"],
  "env": {
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "globals": {
    "ags": "readonly",
    "ARGV": "readonly",
    "imports": "readonly",
    "print": "readonly",
    "console": "readonly",
    "logError": "readonly"
  },
  "rules": {
    "prettier/prettier": "error"
  }
}
